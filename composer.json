{
    "name": "tonystore/livewire-interactions",
    "description": "Package to generate interactions in a Laravel application with Livewire.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel-follow"
    ],
    "require": {
        "php": "^7.2|^8.0",
        "livewire/livewire": "2.*",
        "overtrue/laravel-follow": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Tonystore\\LivewireInteraction\\": "src/"
        },
        "files": [
            "src/Http/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Anthony Medina Sandoval",
            "email": "thony918@outlook.com",
            "homepage": "https://github.com/tony98ms",
            "role": "Developer"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Tonystore\\LivewireInteraction\\LivewireInteractionProvider"
            ],
            "aliases": {
                "LivewireInteraction": "Tonystore\\LivewireInteraction\\LivewireInteractionFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
